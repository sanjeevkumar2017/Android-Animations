<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.telestronix.animation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.telestronix.animation.FadeInActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.telestronix.animation.FadeOutActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.telestronix.animation.CrossfadeActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.telestronix.animation.BlinkActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.telestronix.animation.ZoomInActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.telestronix.animation.ZoomOutActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.telestronix.animation.RotateActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.telestronix.animation.MoveActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.telestronix.animation.SlideUpActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.telestronix.animation.SlideDownActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.telestronix.animation.BounceActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.telestronix.animation.SequentialActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.telestronix.animation.TogetherActivity"
            android:label="@string/app_name" />
    </application>

</manifest>